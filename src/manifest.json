{
    "manifest_version": 3,
    "version": "1.0.0",
    "name":"weekdayhq",
    "description":"weekday stuff",
    "permissions": [
        "tabs",
        "bookmarks",
        "activeTab",
        "webNavigation"
      ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": { 
        "default_popup": "index.html"
    },
    "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self' "
    }
}