{
    "manifest_version": 3,
    "version": "1.0.0",
    "name":"weekdayhq",
    "description":"weekday stuff",
    "permissions": [
        "tabs",
        "bookmarks",
        "activeTab",
        "webNavigation"
      ],
      "background": {
        "scripts": [
            "background.js",
            "runtime.js"
        ]
    },
      "action": { 
          "default_popup": "index.html"
      }
}